{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["sort-keys-fix"],
    "rules": {
        "no-console": "error",
        "quotes": [
            "error",
            "single"
        ],
        "key-spacing":[
            "warn",
            {"align": "colon"}
        ],
        "sort-keys-fix/sort-keys-fix":"warn"
    }
}
